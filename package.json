{
  "name": "sav-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --ignore \"**/*.test.ts\"  --out-dir dist --copy-files --no-copy-ignored",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --ignore-watch src/database  --ignore-watch src/tests src/server.ts",
    "start:dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "migration:seed": "yarn typeorm migration:run && yarn seed:run",
    "test": "jest --runInBand --onlyChanged  --passWithNoTests",
    "test:all": "jest --runInBand  --passWithNoTests",
    "heroku-postbuild": "npx typeorm schema:drop && npx typeorm migrations:run && node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "5.0.0",
    "class-transformer": "^0.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.12.0",
    "date-fns-tz": "^1.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.0",
    "get-stream": "^6.0.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.2",
    "pdfkit": "^0.11.0",
    "pg": "^8.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.0.7",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.171",
    "@types/multer": "^1.4.2",
    "@types/pdfkit": "^0.11.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "git-commit-msg-linter": "^3.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.1.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
